<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="annee_list_view">
      <field name="name">stat par an list</field>
      <field name="model">anneevente</field>
      <field name="arch" type="xml">
        <tree default_order='annee desc'>
          <field name="annee"/>
          <field name="annee_team"/>
          <field name="annee_chifre_ob"/>
          <field name="annee_chifre_aff"/>
           <field name="annee_comer_ob"/>  
          <field name="annee_comer"/> 
          <field name="annee_reel_ob"/> 
          <field name="annee_reel"/> 
          <field name="annee_livraison_ob"/> 
          <field name="annee_livraison"/> 
          <field name="anee_contrat_ob"/>
          <field name="anee_contrat"/>
          <field name="annee_client_ob"/>
          <field name="annee_client"/>
          <field name="annee_materiels_ob"/> 
          <field name="annee_materiels"/> 
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="anne_form_view">
      <field name="name">stat par an form</field>
      <field name="model">anneevente</field>
      <field name="arch" type="xml">
        <form class="o_purchase_order">
              <header>
                <button name="refrech_po" type="object" string="RafraÃ®chir" icon="fa-check" class ="oe_highlight"  />
              </header>
         
              <group>
                <group>
                    <group>
                       <field name="annee"/>
                    </group>
                    <group>
                      <field name="annee_team"/>
                    </group>
                  </group>
              </group>
              <group>
               <group>
                   <group>
                    <field name="mois"  nolabel="1"  widget="one2many"/>
                   </group>
               </group>
              </group>
          
        </form>
      </field>
    </record>
  <record model="ir.actions.act_window" id="stat_annee_window">
      <field name="name">Rapports</field>
      <field name="res_model">anneevente</field>
      <field name="view_mode">tree,form</field>
  </record>
  <menuitem name="Rapports commerciaux" id="affiche_stat_action" parent="sale.sale_menu_root" 
              action="stat_annee_window" sequence="200"/>
</data>
</odoo>
